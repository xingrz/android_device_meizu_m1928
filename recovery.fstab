# mount point   fstype  device                  device2             flags
/boot           emmc    /dev/block/bootdevice/by-name/boot
/cache          ext4    /dev/block/bootdevice/by-name/cache
/recovery       emmc    /dev/block/bootdevice/by-name/recovery      flags=backup=1
/system         ext4    /dev/block/bootdevice/by-name/system
/system_image   emmc    /dev/block/bootdevice/by-name/system
/vendor         ext4    /dev/block/bootdevice/by-name/vendor        flags=backup=1
/vendor_image   emmc    /dev/block/bootdevice/by-name/vendor
/misc           emmc    /dev/block/bootdevice/by-name/misc
/custom         ext4    /dev/block/bootdevice/by-name/custom        flags=display="Custom"

# Data
/data           ext4    /dev/block/bootdevice/by-name/userdata      flags=fileencryption=ice
/firmware       vfat    /dev/block/bootdevice/by-name/modem         flags=display="Firmware";mounttodecrypt;fsflags=ro

# NV
/persist        emmc    /dev/block/bootdevice/by-name/persist       flags=backup=1;display="Persist"

# Removable storage
/usb-otg        vfat    /dev/block/sda1         /dev/block/sda      flags=storage;wipeingui;removable
